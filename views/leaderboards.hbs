<html>
<head>
    <title>PicFive</title>
    <link rel="shortcut icon" type="image/x-icon" href="../images/favicon.png" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u"
    crossorigin="anonymous">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa"
    crossorigin="anonymous"></script>
    <style>
    * {
        position: relative;
    }

    .index_1 {
        text-align: center;
    }
    body {
       /* background-image: url("../images/background.jpg"); */
        background-color:white;
    }

    #title {
        font-size: 80px;
        font-style: normal;
        text-align: center;
    }

    .button,
    .upload {
        font-family: sans-serif;
        padding: 3px 8px;
        font-size: 50px;
        text-align: center;
        cursor: pointer;
        outline: none;
        color: #fff;
        background-color: #4CAF50;
        border: none;
        border-radius: 5px;
        box-shadow: 0 6px #999;
        margin-top: 5px;
        margin-bottom: 10px;
    }

    .button:hover, .upload:hover {
        background-color: #3e8e41;
    }

    .button:active,
    .upload:active {
      background-color: #ff0066;
      box-shadow: 0 4px #666;
      transform: translateY(2px);
    }




    upload,
    .upload input {
        height: 132px;
        width: 132px;
        text-align: center;
    }



    .upload{
        border-radius: 132px;
        width: 132px;
        background: transparent url("../../images/cam.png") center center no-repeat;

    }

    .upload input {
        opacity: 0;
       }
    input[type=file] {
        padding:5px;
        border:2px solid #ccc;
        -webkit-border-radius: 5px;
        border-radius: 5px;
    }


    input[type=submit] {
        padding:5px 15px;
        background:#ccc;
        border:0 none;
        cursor:pointer;
        -webkit-border-radius: 5px;
        border-radius: 5px;
    }

    .select input {
        opacity: 0;
    }


    form.upload2 {
        width: 157px;
        height: 57px;
        background: url(https://lh6.googleusercontent.com/-dqTIJRTqEAQ/UJaofTQm3hI/AAAAAAAABHo/w7ruR1SOIsA/s157/upload.png);
        overflow: hidden;
    }

    form.upload2 [name=select]  {
        display: block !important;
        width: 157px !important;
        height: 57px !important;
        opacity: 0 !important;
        overflow: hidden !important;
    }

    .col {
        padding-right: 10px;
        padding-left: 10px;
        padding-top: 10px;
        padding-bottom: 10px;
    }

    #navBar {
        padding-right: 10px;
        padding-left: 10px;
        padding-top: 0px;
        padding-bottom: 0px;

    }

    img,
    .rows,
    .cols,
    .button{
        max-width: 100%;
        max-height: 100%;
    }

    .listFont {
        font-size: 36px;
    }

    #up{
        font-size: 42px;
    }

    .center-text {
        text-align: center;
    }
    .count {
        font-size: 100px;
    }

    </style>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.6/handlebars.js"></script>
    <script>
    $(function () {
      // Grab the template script
      var theTemplateScript = $("#address-template").html();

      // Compile the template
      var theTemplate = Handlebars.compile(theTemplateScript);

      // Define our data object
      var context={
        mainscore: "1000",
        winner1: "Mo",
        score1: "420",
        winner2: "Mo",
        score2: "420",
        winner3: "Mo",
        score3: "420",
        winner4: "Mo",
        score4: "420",
        winner5: "Mo",
        score5: "420",
        winner6: "Mo",
        score6: "420",
        winner7: "Mo",
        score7: "420",
        winner8: "Mo",
        score8: "420",
        winner9: "Mo",
        score9: "420",
        winner10: "Mo",
        score10: "420"
      };

      // Pass our data to the template
      var theCompiledHtml = theTemplate(context);

      // Add the compiled html to the page
        $('.leaderboardData').html(theCompiledHtml);
        $('.count').each(function () {
        $(this).prop('Counter',0).animate({
            Counter: $(this).text()
        }, {
            duration: 2500,
            easing: 'swing',
            step: function (now) {
                $(this).text(Math.ceil(now));
            }
        });
    });
    });
    </script>
</head>

<body>
    <script id="address-template" type="text/x-handlebars-template">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xs-1">
                </div>
                <div class="col-xs-10">
                    <h1 id="title">PicFive</h1>
                </div>
                <div class="col-xs-1 bg-info">
                    <img src="../images/profile.png" alt="title">
                </div>
            </div>
        </div>
        <div class="center-text">
            <h1> Your score:</h1>
            <div class="count"> {{mainscore}} </div>
            <p> Did you make it on the leaderboards? Here are the best scores: </p>
        </div>
<table class="table table-hover">
  <thead>
    <tr>
      <th>#</th>
      <th>Name</th>
      <th>Score</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td>{{winner1}}</td>
      <td>{{score1}}</td>
    </tr>
    <tr>
      <th scope="row">2</th>
      <td>{{winner2}}</td>
      <td>{{score2}}</td>
    </tr>
    <tr>
      <th scope="row">3</th>
      <td>{{winner3}}</td>
      <td>{{score3}}</td>
    </tr>
    <tr>
      <th scope="row">4</th>
      <td>{{winner4}}</td>
      <td>{{score4}}</td>
    </tr>
    <tr>
      <th scope="row">5</th>
      <td>{{winner5}}</td>
      <td>{{score5}}</td>
    </tr>
    <tr>
      <th scope="row">6</th>
      <td>{{winner6}}</td>
      <td>{{score6}}</td>
    </tr>
    <tr>
      <th scope="row">7</th>
      <td>{{winner7}}</td>
      <td>{{score7}}</td>
    </tr>
    <tr>
      <th scope="row">8</th>
      <td>{{winner8}}</td>
      <td>{{score8}}</td>
    </tr>
    <tr>
      <th scope="row">9</th>
      <td>{{winner9}}</td>
      <td>{{score9}}</td>
    </tr>
    <tr>
      <th scope="row">10</th>
      <td>{{winner10}}</td>
      <td>{{score10}}</td>
    </tr>
  </tbody>
</table>
    </script>
    <div class="leaderboardData"></div>
</body>

</html>
